@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage
@{
	Layout = null;
	ViewBag.Title = Model.Value("pageTitle");
	
}



<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8">
		<meta name="viewport" content="width-device-width, initial-scale-1.0">
		
		<link rel="stylesheet" href="https://kit.fontawesome.com/a576612222.css" crossorigin="anonymous">
		<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
		<link rel="stylesheet" href="/css/main.css">
		
	</head>
	<body>
		<div class="wrapper">
			
				@await Html.PartialAsync("partials/static/topbar.cshtml")
				@await Html.PartialAsync("partials/static/header.cshtml")
				
				@await Html.PartialAsync("partials/static/breadcrumb.cshtml")
				@await Html.PartialAsync("partials/static/contact-info.cshtml")
				
				
				<section class="guide">
					<div class="container">
						<div class="req-text">
							<h6>Request a call back</h6>
							<h2>Let us know about your next project</h2>
							<p>Lorem ipsum dolor sit amet, consectetur adipiscing elit. Maecenas consectetur, ligula semper sagittis lobortis, eros metus efficitur purus</p>
						</div>
				
					<form onsubmit="hanldeContactFormSubmit(event)" novalidate>
						<div class="req-form">
							<div class="inp-one">
								<div class="form-input-group">
									
									<input id="name" name="name" type="text" placeholder="Name" class="form-control">
									
								</div>
								<div class="form-input-group">
									
									<input id="email" name="email" type="email" placeholder="Enter your e-mail address" class="form-control">
									
								</div>
							</div>
							<div class="inp-two">
								<div class="form-input-group">
									
									<input id="message" name="message" placeholder="Enter your message..." class="form-control">
									
								</div>
								<div class="form-input-group">
									
									<select class="form-control">
										<option>Financial consulting</option>
										<option>bla bla</option>
										<option>bla bla</option>
									</select>
								</div>
							</div>
						</div>
						
						
						<input type="submit" class="form-button btn-submit" value="Submit" />
					</form>
					</div>
				</section>
				
			
		@await Html.PartialAsync("partials/static/footer.cshtml")
		</div>
		
		<script src="/scripts/main.js"></script>
		 
	</body>
</html>
